---
import BaseLayout from "./BaseLayout.astro";
const { frontmatter } = Astro.props
---
<BaseLayout>
    <h1>
        {frontmatter.title}
    </h1>
    <img src=`${frontmatter.hero.src}` alt="">
    <div>
        {frontmatter.pubDate}
    </div>
    <div>
        {frontmatter.author}
    </div>
</BaseLayout>